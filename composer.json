{
    "name": "psycho/groups",
    "description": "Laravel 5 user groups package originally by musonza/groups. I fork it and made some upgrades in the code & mold it according to my project needs.",
    "license": "MIT",
    "keywords": [
        "laravel",
        "groups",
        "conversations"
    ],
    "authors": [
        {
            "name": "Zeeshan",
            "email": "zeeshanbutt223@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "laravel/framework": "^6.2.42|^8.40",
        "orchestra/testbench": "6.x-dev",
        "orchestra/database": "6.x-dev",
        "mockery/mockery": "^1.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "Psycho\\Groups\\": "src/"
        },
        "classmap": [
            "tests/GroupsTestCase.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Psycho\\Groups\\GroupsServiceProvider"
            ]
        }
    }
